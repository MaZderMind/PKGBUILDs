# Maintainer: Sebastian Reimers <service@it-sreimers.de>
pkgname=baresip
pkgver=0.4.7
pkgrel=1
arch=(armv7h)
url="http://www.creytiv.com/baresip.html"
license=('BSD license')
depends=(glibc)
source=(http://www.creytiv.com/pub/$pkgname-$pkgver.tar.gz)
md5sums=('2583e1170b6656ab4dacf714954e66a1')

build() {
    cd $pkgname-$pkgver
    make
}

package() {
    cd $pkgname-$pkgver
    make DESTDIR="$pkgdir" install
}
