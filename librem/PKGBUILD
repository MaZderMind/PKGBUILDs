# Maintainer: Sebastian Reimers <service@it-sreimers.de>
pkgname=librem
pkgver=0.4.4
pkgrel=1
arch=(armv7h)
url="http://www.creytiv.com/baresip.html"
license=('BSD license')
depends=('libre')
makedepends=('libre')
source=(http://www.creytiv.com/pub/rem-$pkgver.tar.gz)
md5sums=('62205b32f92f83e24b8cb199836acad6')

build() {
    cd rem-$pkgver
    make
}

package() {
    cd rem-$pkgver
    make DESTDIR="$pkgdir" install
}
